  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "files": [
    "dist"
  ],
  "type": "module",
  "author": "allegrormc@gmail.com",
  "license": "ISC",
  "devDependencies": {
    "@peculiar/webcrypto": "^1.4.6",
    "@types/node": "^22.7.5",
    "@types/uuid": "^10",
    "eslint": "^9.11.1",
    "jest": "^29.2.5",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.1.0",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@automerge/automerge": "^3.2.4",
    "@collabswarm/collabswarm": "workspace:packages/collabswarm",
